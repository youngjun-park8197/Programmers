!! 주어진 문제 합격과 일부 합격 답

#include <string>
#include <vector>
#include <algorithm>
#include <iostream>

using namespace std;

long long solution(long long n) {
    long long answer = 0;
    int num = 0;
    
    string temp = to_string(n);
    
    sort(temp.begin(), temp.end(), greater<char>());
    
    answer = stoi(temp);
    
    return answer;
}

